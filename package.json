{
  "name": "goodreads-page-converter",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "clean": "rimraf build",
    "build:react": "react-build-info && react-scripts build",
    "watch:react": "react-build-info && react-scripts start",
    "build": "npm-run-all clean build:react",
    "start": "npm-run-all clean watch:react",
    "pbuild": "npm version patch && react-build-info && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.3",
    "react": "19.0.0",
    "react-bootstrap": "^2.0.3",
    "react-dom": "19.0.0",
    "react-scripts": "^5.0.1",
    "sweetalert2": "^11.15.10",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "react-build-info": "^0.0.2",
    "rimraf": "^6.0.1"
  }
}
